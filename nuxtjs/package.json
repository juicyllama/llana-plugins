{
  "name": "@juicyllama/llana-plugins-nuxtjs",
  "version": "1.4.0",
  "description": "NuxtJs Plugin for Llana.",
  "tags": [
    "llana",
    "nuxtjs",
    "plugin"
  ],
  "keywords": [
    "llana",
    "nuxtjs",
    "plugin"
  ],
  "author": {
    "name": "JuicyLlama Studio",
    "email": "studio@juicyllama.com",
    "url": "https://juicyllama.com"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/juicyllama/llana-plugins.git"
  },
  "bugs": "https://github.com/juicyllama/llana-plugins/issues",
  "license": "BSD-4-Clause",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "build": "nuxt-module-build prepare && nuxt-module-build build",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.13.2",
    "socket.io-client": "^4.8.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.6.0",
    "@nuxt/eslint-config": "^0.6.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.13.2",
    "@nuxt/test-utils": "^3.14.4",
    "@types/node": "latest",
    "changelogen": "^0.5.7",
    "eslint": "^9.13.0",
    "nuxt": "^3.13.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 4,
    "useTabs": true,
    "semi": false,
    "printWidth": 120,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "bracketSameLine": true
  }
}
